#include <stdio.h>
#include <stdlib.h>

int main()
{
	int n, i;

	printf("n = ");
	scanf("%d", &n);

	double pi = 0.;
	//TODO
	//add code below 
    int p = 1;
	for (i = 0; i <= n; i++)
    {


        pi += ((4.0/(8.0*i+1.0)) - (2.0/(8.0*i+4.0)) - (1.0/(8.0*i+5.0))-(1.0/(8.0*i+6.0)))/p;
        p *= 16;
        
    }


	printf("PI = %.10f\n", pi);
	return 0;
}
